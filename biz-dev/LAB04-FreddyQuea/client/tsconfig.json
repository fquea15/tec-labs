{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "sourceMap": true,
    "outDir": "./dist",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@ui/*": [
        "src/components/ui/*"
      ],
      "@layout/*": [
        "src/components/layout/*"
      ],
      "@product/*": [
        "src/components/product/*"
      ],
      "@cart/*": [
        "src/components/cart/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}
